{
  "name": "zzt/framework",
  "type": "library",
  "version": "0.1.0",
  "description": "The zzt php framework",
  "keywords": [
    "framework",
    "php",
    "dev"
  ],
  "homepage": "http://www.gihub.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Cristian Cornea",
      "email": "contact@corneascorner.dev",
      "homepage": "https://www.corneascorner.dev"
    }
  ],
  "autoload": {
    "psr-4": {
      "zzt\\": "/lib"
    },
    "exclude-from-classmap": [
      "**/tests/"
    ],
    "files": [
      "globals/router.php",
      "globals/tools.php"
    ]
  },
  "autoload-dev": {},
  "require": {
    "php": ">=8.3",
    "jenssegers/blade": "^2.0"
  },
  "config": {
    "preferred-install": "dist"
  }
}
